#!/bin/sh
#
# These are the steps I typically use to configure and compile Bochs
# on a Win32 system with cygwin (v.20.1)
#

CC="gcc"
CXX="g++"
CFLAGS="-O6 -march=pentium"
CXXFLAGS="$CFLAGS"

export CC
export CXX
export CFLAGS
export CXXFLAGS

./configure --enable-cdrom \
            --enable-sb16=win \
	    --enable-ne2000 \
	    --enable-vbe \
		--enable-all-optimizations
