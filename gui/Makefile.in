# Makefile for the gui component of bochs


@SUFFIX_LINE@

srcdir = @srcdir@
VPATH = @srcdir@

SHELL = /bin/sh

@SET_MAKE@

CXX = @CXX@
CXXFLAGS = @CXXFLAGS@  @GUI_CXXFLAGS@

LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
X_CFLAGS = @X_CFLAGS@
BEOS_CFLAGS = -Wno-multichar
RANLIB = @RANLIB@

GUI_OBJS_X11  = x.o
GUI_OBJS_SDL = sdl.o
GUI_OBJS_BEOS = beos.o
GUI_OBJS_WIN32 = win32.o
GUI_OBJS_MACOS = macintosh.o
GUI_OBJS_CARBON = carbon.o
GUI_OBJS_NOGUI = nogui.o
GUI_OBJS_TERM  = term.o
GUI_OBJS_RFB = rfb.o
GUI_OBJS_AMIGAOS = amigaos.o
GUI_OBJS_WX = wx.o wxmain.o wxdialog.o
GUI_OBJS_NON_PLUGINABLE = keymap.o gui.o siminterface.o
GUI_OBJS_PLUGINABLE = @GUI_OBJS@
GUI_OBJS_EXTERNAL =

GUI_OBJS_NON_PLUGINS=@GUI_NON_PLUGINS_OBJ@
GUI_OBJS_PLUGINS=@GUI_PLUGINS_OBJ@
GUI_OBJS = $(GUI_OBJS_NON_PLUGINS)
GUI_SO_OBJS = $(GUI_OBJS_PLUGINS)
OBJS = $(GUI_OBJS_NON_PLUGINS) $(GUI_OBJS_PLUGINS) $(GUI_OBJS_EXTERNAL)

BX_INCDIRS = -I.. -I$(srcdir)/.. -I../iodev -I$(srcdir)/../iodev -I../@INSTRUMENT_DIR@ -I$(srcdir)/../@INSTRUMENT_DIR@
LOCAL_CXXFLAGS =

#
# -------- end configurable options --------------------------
#

BX_GUI_OBJS = $(GUI_OBJS)


BX_INCLUDES = ../iodev/iodev.h control.h siminterface.h

.@CPP_SUFFIX@.o:
	$(CXX) @DASH@c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) @CXXFP@$< @OFP@$@


all: libgui.a plugins

libgui.a: $(BX_GUI_OBJS)
	@RMCOMMAND@ libgui.a
	@MAKELIB@ $(BX_GUI_OBJS)
	$(RANLIB) libgui.a

$(BX_GUI_OBJS) : $(BX_INCLUDES)

beos.o: beos.@CPP_SUFFIX@
	$(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) $(BEOS_CFLAGS) $<

x.o: x.@CPP_SUFFIX@
	$(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) $(X_CFLAGS) $<

%.so: %.o
	$(CXX) -shared  $< -o $@
	mkdir -p ../plugins
	cp $@ ../plugins

# special rule for wx, since it has 3 files
wx.so: $(GUI_OBJS_WX)
	$(CXX) -shared $(GUI_OBJS_WX) -o $@ `@WX_CONFIG@ --cxxflags`
	mkdir -p ../plugins
	cp $@ ../plugins

plugins: $(GUI_SO_OBJS:.o=.so)

clean:
	@RMCOMMAND@ *.o
	@RMCOMMAND@ *.a

dist-clean: clean
	@RMCOMMAND@ Makefile

