-    BX_INSTR_FETCH_DECODE_COMPLETED(BX_CPU_ID, i);
+    bxInstruction_c *i = &iStorage;
+    unsigned length = 1, n;
+ 
+#if BX_SUPPORT_ICACHE
+    bxICacheEntry_c *trace = fetchInstructionTrace(&iStorage, eipBiased);
+    if (trace) {
