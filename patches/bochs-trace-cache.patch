-         BX_CPU_THIS_PTR sregs[BX_SEG_REG_CS].cache.u.segment.d_b, Is64BitMode());
-#endif
+    boundaryFetch(fetchPtr, remainingInPage, i);
   }
 
   return i;
 }
 
+#endif
